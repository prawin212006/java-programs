import java.util.Scanner;
public class Main
{
public static void main(String[] args) {
   Scanner src=new Scanner(System.in);
   int a= src.nextInt();
   int b= src.nextInt();
   int c= src.nextInt();
   if(a<b && a<c){
       System.out.print(a);
   }
   else if(b<c) {
       System.out.print(b);
   }
   else {
       System.out.print(c);
   }
}
