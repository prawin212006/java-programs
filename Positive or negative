import java.util.Scanner;
public class Main

{
public static void main(String[] args) {
   Scanner sc=new Scanner(System.in);
   int a=sc.nextInt();
   int b=sc.nextInt();
   int product=a*b;
   if(product>0){
       System.out.print("positive");
   }
   else if(product<0){
       System.out.print("negative");
   }
   else{
       System.out.print("zero");
   }
}
}
